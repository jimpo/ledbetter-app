{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 14",

	"compilerOptions": {
		"outDir": "dist",
		"lib": ["es2020"],
		"module": "es2020",
	  	"moduleResolution": "node",
		"target": "es2020",

		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},

  	"exclude": ["assemblyscript/**/*"]
}
